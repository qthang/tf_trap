#!name = Tự động tham gia TestFlight
#!desc = Tự động theo dõi hạn ngạch TestFlight và thêm hạn ngạch mới ngay khi có sẵn.
#!openUrl = 
#!author = QThang net
#!homepage = https://gitlab.com/lodepuly/vpn_tool/-/tree/master/Tool/Loon/Plugin
#!icon = https://gitlab.com/lodepuly/iconlibrary/-/raw/main/App_icon/120px/TestFlight.png
#!input = APP_ID
#!date = 2024-03-07 22:10:30

[Rule]
DOMAIN, testflight.apple.com, PROXY

[Script]
http-request ^https?:\/\/testflight\.apple\.com\/v3\/accounts\/.*\/apps$ script-path = https://cdn.jsdelivr.net/gh/qthang/tf_trap@5b893dd759d329ac7a5387d24bdb04fab2960629/TF_keys.js, tag = TF信息获取
http-request ^https://testflight.apple.com/join/(.*) script-path = https://cdn.jsdelivr.net/gh/qthang/tf_trap@5b893dd759d329ac7a5387d24bdb04fab2960629/TF_keys.js, tag = APP_ID获取
cron "*/1 * * * * *" script-path = https://cdn.jsdelivr.net/gh/qthang/tf_trap@5b893dd759d329ac7a5387d24bdb04fab2960629/Auto_join_TF.js, tag = 自动加入TF, timeout = 180

[Mitm]
hostname = testflight.apple.com
